const start = clock()

var person = {
    first: "John",
    last: "Smith",
    age: 45,
    nums: [1, 2, 3, 4],
    getName: () => f"${this.first} ${this.last}",
    addAge: (n) => {
        this["age"] += n
        return this
    }
}

person.addAge(10).addAge(10)

print(person)

print("Duration: ", clock() - start)

