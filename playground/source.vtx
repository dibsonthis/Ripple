import [] : "time"

const somefunc = () => {
    var l = 0
    for (0..100, i) {
        l += i
    }
}

const average = (list) => {
    var sum = 0
    var len = list.length()
    for (list, i, v) {
        sum += v
    }
    return sum / len
}

var times = []

for (0..10, i) {
    times[100] = time(somefunc)
}

var avg = average(times)

print("Average: ", avg, "\n")

