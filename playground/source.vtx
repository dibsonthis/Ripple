const start = clock()

var person = {
    first: "John",
    last: "Smith",
    age: 45,
    nums: [1, 2, 3, 4],
    getName: () => f"${this.first} ${this.last}",
    addAge: (n) => {
        this["age"] += n
        return this
    }
}

const addNums = (person, n) => {
    person.nums[100] = n
    return person
}

person.addNums(10).print()

print("Duration: ", clock() - start, "\n")

