const Timer = (func) => {
    return (...args) => {
        try {
            10 * ""
        } catch (e) {
            println(e)
        }
        const start = clock()
        var value = func(...args)
        const duration = clock() - start
        println(f"Duration (${func.info().name}): ", duration)
        return value
    }
}

@Timer
const somefunc = () => {
    var i = 0
    for (0..100) {
        for (0..100) {
            i += 1
        }
    }
}

somefunc()