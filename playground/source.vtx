import requests

const get_response = () => {
    const th = threading.thread(() => requests.get("https://24pullrequests.com", "/projects.json"))
    while (!th.ready()) {
        print("Loading Inner...\r")
    }
    ret th.get()
}

const data = threading.thread(get_response)

while (!data.ready()) {
    print("Loading Outer...\r")
}

const res = data.get()

println(res.toJson())