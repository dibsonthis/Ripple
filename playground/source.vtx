const start = clock()

var x = 10

const addOne = (x) => {
    return x += 1
}

const prependText = (value, message) => {
    return message + string(value)
}

x
.addOne()
.addOne()
.addOne()
.prependText("Value of 'x': ")
.print("\n")

print("Duration: ", clock() - start, "\n")

