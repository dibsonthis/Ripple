enum ColorCode {
    HEADER: "\033[95m",
    OKBLUE: "\033[94m",
    OKCYAN: "\033[96m",
    OKGREEN: "\033[92m",
    WARNING: "\033[93m",
    FAIL: "\033[91m",
    ENDC: "\033[0m",
    BOLD: "\033[1m",
    UNDERLINE: "\033[4m"
}

const log = (message: String, code: ColorCode) => {
	print(code)
	println(message)
	print(ColorCode.ENDC)
}

const log = (message: String) => {
	println(message)
}

const warningGlyph = "\xE2\x9A\xA0"
const errorGlyph = "\xE2\x9D\x8C"
const debugGlyph = "\xF0\x9F\x90\x9E"
const ehGlyph = "\xF0\x9F\x98\x92"

log(f"CRITICAL ERROR ${errorGlyph}", ColorCode.FAIL)
log(f"WARNING ${warningGlyph}", ColorCode.WARNING)
log(f"DEBUG ${debugGlyph}", ColorCode.OKBLUE)
log(f"EH ${ehGlyph}")