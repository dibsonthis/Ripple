import [] : string

const last = (x: List) => x[x.length-1]

const getExt = (str: String) => str.split(".").last() 

type CSVPath = (x: String) => getExt(x) == "csv"
type TXTPath = (x: String) => getExt(x) == "txt"

const open = (fileType: CSVPath) => println("Opening \"csv\" file...")
const open = (fileType: TXTPath) => println("Opening \"txt\" file...")
const open = (fileType) => println(f"Cannot open \"${getExt(fileType)}\" files.")

open("blah.csv")

