type PosNum = (num: Number) => num > 0
type NegNum = (num: Number) => num < 0

const numfunc = (num: PosNum) => println("number is pos")
const numfunc = (num: NegNum) => println("number is neg")
const numfunc = (num: Number) => println("number is 0")

